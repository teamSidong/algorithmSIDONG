1. 옷가게할인받기
function solution(price) {
    var answer = 0;
    if(price>=500000){
         answer =price - price*0.2 
    }else if(300000>= price >=500000){
        answer =price - price*0.1
    }else if(price >=100000){
        answer =price - price*0.05
    }
    
    return answer;
}
2. 내적
function solution(a, b) {
    var answer = 0;
    for(let i=0; i<a.length; i++){
        answer += a[i]*b[i]
    }
    return answer;
}
